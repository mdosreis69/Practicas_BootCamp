
USE SPRINGFIELD;

CREATE TABLE PERSONAJES
(ID_PERSONAJE INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_PERSONAJE VARCHAR(255),
ID_FAMILIA INT,
ID_SEXO INT,
ID_MATRIMONIO INT,
ID_LUGAR_RESIDENCIA INT,
ID_PROFESION INT,
ID_LUGAR_PROFESION INT,
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE MASCOTAS
(ID_MASCOTA INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_MASCOTA VARCHAR(255),
ID_FAMILIA INT,
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE FAMILIAS
(ID_FAMILIA INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_FAMILIA VARCHAR(255),
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE PROFESIONES
(ID_PROFESION INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_PROFESION VARCHAR(255),
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE SEXO
(ID_SEXO INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_SEXO VARCHAR(255),
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE MATRIMONIOS
(ID_MATRIMONIO INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_MATRIMONIO VARCHAR(255),
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE LUGARES
(ID_LUGAR INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_LUGAR VARCHAR(255),
ID_CIUDAD INT,
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE CIUDADES
(ID_CIUDAD INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_CIUDAD VARCHAR(255),
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

CREATE TABLE FRASES
(ID_FRASE INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
DESC_FRASE VARCHAR(2048),
ID_PERSONAJE INT,
FECHA_INSERT DATETIME,
FECHA_MODIFICACION DATETIME);

